{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/aug.rar",
      "dest": "/public/aug.rar",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Content-Disposition": "attachment; filename=aug.rar"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
