{
  "rewrites": [
    {
      "source": "/aug.rar",
      "destination": "/public/aug.rar"
    }
  ],
  "headers": [
    {
      "source": "/aug.rar",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Content-Disposition", 
          "value": "attachment; filename=aug.rar"
        }
      ]
    }
  ]
}
